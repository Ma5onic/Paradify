{
  "manifest_version": 2,
  "name": "Paradify",
  "short_name": "Paradify",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "2.7",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.11.1.min.js",
        "bg.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Songs to your Spotify",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}